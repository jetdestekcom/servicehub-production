# Production Environment Variables - JETDESTEK Platform
# SECURITY WARNING: This file contains sensitive information
# Never commit this file to version control

# Application
NODE_ENV=production
NEXTAUTH_URL=https://jetdestek.com
NEXTAUTH_SECRET=your-super-secure-nextauth-secret-here-min-32-chars
APP_URL=https://jetdestek.com

# Database - PostgreSQL (Production)
DATABASE_URL=postgresql://username:password@localhost:5432/jetdestek_prod

# Security Keys
JWT_SECRET=your-super-secure-jwt-secret-here-min-32-chars
ENCRYPTION_KEY=your-super-secure-encryption-key-here-min-32-chars

# Google OAuth (Production)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Stripe (Production)
STRIPE_SECRET_KEY=sk_live_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_live_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-stripe-webhook-secret

# AWS S3 (Production)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=eu-central-1
AWS_S3_BUCKET=jetdestek-prod

# Email (Production)
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USER=apikey
SMTP_PASS=your-sendgrid-api-key
SMTP_FROM=noreply@jetdestek.com

# SMS (Production)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Google Maps (Production)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Redis (Production)
REDIS_URL=redis://localhost:6379

# Monitoring (Production)
SENTRY_DSN=your-sentry-dsn
LOG_LEVEL=warn

# Security
SECURITY_ALERT_WEBHOOK=your-security-webhook-url
SIEM_ENDPOINT=your-siem-endpoint

